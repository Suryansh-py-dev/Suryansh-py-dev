a = int(input("Enter first number: "))
b = int(input("Enter second number: "))
op = input("The operation is (+, -, *, /): ")

if op == '+':
    result = a + b
elif op == '-':
    result = a - b
elif op == '*':
    result = a * b
elif op == '/':
    if b != 0:
        result = a / b
    else:
        result = "Cannot divide by zero"
else:
    result = "Invalid operation"

print("Result:", result)